<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>

  <chase-distance-m type="double" archive="y"> -100 </chase-distance-m>

  <view n="100">
    <name>Copilot View</name>
    <internal archive="y">true</internal>
    <type>lookfrom</type>
    <config>
      <from-model type="bool">true</from-model>
      <ground-level-nearplane-m type="double">0.5f</ground-level-nearplane-m>
      <x-offset-m archive="y">   0.46849 </x-offset-m> <!-- Right -->
      <y-offset-m archive="y">  -1.87182 </y-offset-m> <!-- Up    -->
      <z-offset-m archive="y"> -20.88196 </z-offset-m> <!-- Back  -->
      <pitch-offset-deg> -15 </pitch-offset-deg>
      <default-field-of-view-deg> 65 </default-field-of-view-deg>
    </config>
  </view>

