<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>

  <fx>
 
    <!-- Moteur 1 -->
    <crank>
      <name>engstart</name>
      <path>Aircraft/Saunders-Roe-Princess/Sounds/starter.wav</path>
      <property>/engines/engine[0]/running</property>
      <volume>
        <offset>-0.50</offset>
      </volume>
      <pitch>
        <property>/engines/engine[0]/running</property>
        <factor>1.0</factor>
      </pitch>
      <reference-dist>100</reference-dist>
      <max-dist>1000</max-dist>
    </crank>

    <engine>
      <name>enginerun</name>
      <mode>looped</mode>
      <path>Aircraft/Saunders-Roe-Princess/Sounds/running.wav</path>
      <condition>
        <property>/engines/engine[0]/running</property>
        <greater-than>
          <property>/engines/engine[0]/rpm</property>
          <value>3</value>
        </greater-than>
      </condition>
      <volume> 
        <property>/orientation/alpha-deg</property>
        <type>log</type>
        <min>1.0</min>
        <max>1.5</max>
      </volume>
      <volume> 
        <property>/sim/current-view/internal</property>
        <offset>1.0</offset>
        <factor>-0.4</factor>
      </volume>
      <pitch> 
        <property>/engines/engine[0]/rpm</property>
        <factor>0.00006</factor>
        <min>0.6</min>
        <max>2.0</max>
        <offset>0</offset>
      </pitch>
      <reference-dist>100</reference-dist>
      <max-dist>1000</max-dist>
    </engine>

