        <property>/velocities/airspeed-kt</property>
        <min>30.0</min>
      </volume>
      <reference-dist>10.0</reference-dist>
      <max-dist>20.0</max-dist>
    </stall>
 
    <ocean-outside>
      <name>water2</name>
      <mode>looped</mode>
      <path>Aircraft/Saunders-Roe-Princess/Sounds/wave.wav</path>   
      <condition>
        <or> 
          <property>/gear/gear[0]/wow</property>
          <property>/gear/gear[1]/wow</property>
          <property>/gear/gear[2]/wow</property>
          <property>/gear/gear[3]/wow</property>
          <property>/gear/gear[4]/wow</property>
        </or>
      </condition>
      <volume>
        <property>position/gear-agl-m</property>
        <factor>-0.0009</factor>
        <offset>0.235</offset>
        <min>0.01</min>
        <max>0.235</max>
      </volume>
    </ocean-outside>

    <ohno>
      <name>crash</name>
      <delay-sec>0</delay-sec>
      <path>Sounds/thunder.wav</path>
      <condition>
        <property>sim/crashed</property>
        <value>true</value>       
      </condition>
      <volume>
        <property>sim/sound/avionics/volume</property>
      </volume>
    </ohno>

